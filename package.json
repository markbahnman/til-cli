{
  "name": "til-cli",
  "version": "0.1.2",
  "description": "Tool to make saving quick TILs",
  "repository": {
    "type": "git",
    "url": "https://github.com/markbahnman/til-cli"
  },
  "homepage": "https://github.com/markbahnman/til-cli",
  "keywords": [
    "til",
    "cli"
  ],
  "bin": {
    "til": "bin/til-cli.js"
  },
  "main": "bin/til-cli.js",
  "preferGlobal": true,
  "scripts": {
    "start": "npm run compile && node ./lib/index.js",
    "lint": "eslint -c .eslintrc src",
    "lint-watch": "esw -c .eslintrc src --watch",
    "test": "mocha --compilers js:babel-register test/",
    "compile": "babel -d lib/ src/"
  },
  "author": "Mark Bahnman <mark.bahnman@gmail.com> (https://github.com/markbahnman)",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "commander": "^2.9.0",
    "inquirer": "^0.12.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-import": "^0.13.0",
    "eslint-watch": "^2.1.7",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3"
  }
}
